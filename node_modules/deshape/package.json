{
  "module": "./esm/mod.js",
  "main": "./script/mod.js",
  "types": "./types/mod.d.ts",
  "name": "deshape",
  "version": "0.0.1",
  "description": "subShape provides primitives and patterns for crafting composable shapes featuring cohesive typing, validation, serialization, and reflection.",
  "sideEffects": false,
  "repository": "github:dedotdev/deshape",
  "exports": {
    ".": {
      "import": {
        "types": "./types/mod.d.ts",
        "default": "./esm/mod.js"
      },
      "require": {
        "types": "./types/mod.d.ts",
        "default": "./script/mod.js"
      }
    },
    "./scale": {
      "import": {
        "types": "./types/scale.d.ts",
        "default": "./esm/scale.js"
      },
      "require": {
        "types": "./types/scale.d.ts",
        "default": "./script/scale.js"
      }
    }
  },
  "devDependencies": {
    "@types/node": "^18.11.9"
  }
}