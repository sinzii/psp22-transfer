{
  "name": "@dedot/api",
  "version": "0.6.1",
  "description": "A delightful JavaScript/TypeScript client for Polkadot & Substrate",
  "author": "Thang X. Vu <thang@coongcrafts.io>",
  "homepage": "https://github.com/dedotdev/dedot/tree/main/packages/api",
  "repository": {
    "directory": "packages/api",
    "type": "git",
    "url": "https://github.com/dedotdev/dedot.git"
  },
  "main": "./cjs/index.js",
  "type": "module",
  "sideEffects": false,
  "dependencies": {
    "@dedot/codecs": "0.6.1",
    "@dedot/providers": "0.6.1",
    "@dedot/runtime-specs": "0.6.1",
    "@dedot/shape": "0.6.1",
    "@dedot/storage": "0.6.1",
    "@dedot/types": "0.6.1",
    "@dedot/utils": "0.6.1"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc --project tsconfig.build.cjs.json",
    "clean": "rm -rf ./dist && rm -rf ./tsconfig.tsbuildinfo ./tsconfig.build.tsbuildinfo",
    "test": "vitest --watch=false"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./cjs/index.js",
      "default": "./index.js"
    },
    "./chaintypes": {
      "types": "./chaintypes/index.d.ts",
      "import": "./chaintypes/index.js",
      "require": "./cjs/chaintypes/index.js",
      "default": "./chaintypes/index.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "devDependencies": {
    "@polkadot/types": "~14.0.1"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "Apache-2.0",
  "gitHead": "90f161f221c4c4479c92f016591c601614594dd9",
  "module": "./index.js",
  "types": "./index.d.ts"
}