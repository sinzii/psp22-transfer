{
  "name": "@dedot/cli",
  "version": "0.6.1",
  "author": "Thang X. Vu <thang@coongcrafts.io>",
  "homepage": "https://github.com/dedotdev/dedot/tree/main/packages/cli",
  "repository": {
    "directory": "packages/cli",
    "type": "git",
