    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.33"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "engines": {
    "node": ">=18"
  },
